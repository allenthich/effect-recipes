{
  "name": "fastify-server",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run dev:db & bun run dev:server",
    "dev:server": "bun --watch ./src/index.ts",
    "dev:db": "docker compose up",
    "db:pregenerate": "npx @better-auth/cli generate --config=./src/services/authentication/auth.ts --output=./src/services/authentication/auth-schema.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "devDependencies": {
    "@effect/language-service": "^0.44.0",
    "@types/bun": "latest",
    "drizzle-kit": "^0.31.5",
    "pino-pretty": "^13.1.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@effect/platform-bun": "^0.81.1",
    "@effect/platform-node": "^0.98.4",
    "@effect/sql": "^0.46.0",
    "@effect/sql-drizzle": "^0.45.0",
    "@effect/sql-pg": "^0.47.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/type-provider-json-schema-to-ts": "^5.0.0",
    "better-auth": "^1.3.31",
    "drizzle-orm": "^0.44.7",
    "fastify": "^5.6.1",
    "fastify-plugin": "^5.1.0",
    "postgres": "^3.4.7"
  }
}
